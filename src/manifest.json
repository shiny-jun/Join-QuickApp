    "versionCode": "1",
    "minPlatformVersion": "101",
    "icon": "/Common/logo.jpg",
    "features": [
        { "name": "system.prompt" },
        { "name": "system.router" },
        { "name": "system.shortcut" }
    ],
    "permissions": [
        { "origin": "*" }
    ],
    "config": {
        "logLevel": "off"
    },
    "router": {
        "entry": "Home",
        "pages": {
